public class Loop_Break{
	public static void main(String[] args){
		boolean isSick = true;  // ture(1)  , false(0) 
		
		for(int i=1; i<=8; i++){
			
			{
				if(i==6)
				   break;
				
				System.out.println("Loop_break time : "+i);
				
				
				
			}
		}
		
		System.out.println("Loop_outside , go home ");
		
	}
}
