public class IfElse_Score {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int score = 85;
		if(score >= 90){
			System.out.println("Grade A");	
		}
		else if(score >= 80){
			System.out.println("Grade B");	
		}
		else if(score >= 70){
			System.out.println("Grade C");	
		}
		else{
			System.out.println("Fail (不及格=.=)");
		}
		
		
		
		
		
	}

}
